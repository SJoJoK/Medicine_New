Page {
  background: rgb(247, 246, 246);
  line-height: 1.6;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
}

icon {
  vertical-align: middle;
}

.tapCard {
  display: flex;
  flex-direction: row;
  justify-content: start;
  height: 65rpx;
  margin-top: 5rpx;
}

.tapCard view {
  width: 50%;
  height: 100%;
  text-align: center;
  font-size: 26rpx;
  line-height: 65rpx;
  border-radius: 10rpx 10rpx 0 0;
}

.tapOn {
  background-color: black;
  color: rgb(255, 255, 255);
}

.tapOff {
  background-color: white;
  color: black;
  border: 1rpx solid black;
  border-bottom: none;
}

.delete {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 13rpx 20rpx;
  height: 50rpx;
  line-height: 50rpx;
}

.myInput {
  height: 60rpx;
  line-height: 60rpx;
  width: 400rpx;
  font-size: 28rpx;
  background-color: white;
  border-radius: 10rpx;
  text-align: start;
}

.Btns {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.downBtn {
  width: 70rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  background-color: black;
  text-align: center;
  border-radius: 10rpx;
  color: white;
  margin: 5rpx;
}

.upBtn {
  width: 70rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  background-color: white;
  text-align: center;
  border-radius: 10rpx;
  color: black;
  border: 1rpx solid black;
  margin: 5rpx;
}

.theList {
  width: 100%;
}

.listItem {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 13rpx 20rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.weui-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 17px;
}

.weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
}

.weui-cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid #d9d9d9;
  color: #d9d9d9;
}

.weui-cells__title {
  margin-top: 0.77em;
  margin-bottom: 0.3em;
  padding-left: 15px;
  padding-right: 15px;
  color: #999;
  font-size: 14px;
}

.weui-cells_after-title {
  margin-top: 0;
}

.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-cell:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
  left: 15px;
}

.weui-cell:first-child:before {
  display: none;
}

.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.weui-cell__ft {
  text-align: right;
  color: #999;
}

.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
}

.weui-label {
  width: 105px;
  word-wrap: break-word;
  word-break: break-all;
}

.weui-input {
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
}

.weui-uploader__bd {
  margin-bottom: -4px;
  margin-right: -9px;
  overflow: hidden;
}

.weui-uploader__file {
  float: left;
  margin-right: 9px;
  margin-bottom: 9px;
}

.weui-uploader__img {
  display: block;
  width: 79px;
  height: 79px;
}

.weui-uploader__input-box {
  float: left;
  position: relative;
  margin-right: 9px;
  margin-bottom: 9px;
  width: 77px;
  height: 77px;
  border: 1px solid #d9d9d9;
}

.weui-uploader__input-box:before, .weui-uploader__input-box:after {
  content: " ";
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.weui-uploader__input-box:before {
  width: 2px;
  height: 39.5px;
}

.weui-uploader__input-box:after {
  width: 39.5px;
  height: 2px;
}

.weui-uploader__input-box:active {
  border-color: #999;
}

.weui-uploader__input-box:active:before, .weui-uploader__input-box:active:after {
  background-color: #999;
}

.weui-uploader__input {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.weui-btn {
  margin: 15px;
}

.weui-btn:first-child {
  margin-top: 0;
}

.weui-btn-area {
  margin: 1.17647059em 15px 0.3em;
}
